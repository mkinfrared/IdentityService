{
  "npmClient": "pnpm",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "lib/**",
        "build/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "updates": {
      "cache": false
    },
    "build-storybook": {
      "dependsOn": [
        "build",
        "^build-storybook"
      ],
      "outputs": [
        "storybook-static/**"
      ]
    },
    "storybook": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    }
  }
}
