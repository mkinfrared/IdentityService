{
  "name": "@identity-service/admin",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env CI=false react-scripts build",
    "test": "cross-env CI=true react-scripts test --collectCoverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "coverage": "open-cli coverage/lcov-report/index.html",
    "updates": "ncu",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@identity-service/core": "workspace:*",
    "@identity-service/ui": "workspace:*",
    "axios": "^0.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "web-vitals": "^2.1.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-docs": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/builder-webpack5": "^6.4.13",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/node-logger": "^6.4.13",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.13",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.19",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "open-cli": "^7.0.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.48.0",
    "storybook-design-token": "^1.4.0",
    "typescript": "^4.5.4"
  },
  "browserslist": {
    "production": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 3 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.stories.{ts,tsx}",
      "!**/*.type.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coveragePathIgnorePatterns": [
      "index",
      "reportWebVitals",
      "react-app-env.d",
      "store.ts",
      "__snapshots__"
    ],
    "transformIgnorePatterns": [
      "@identity-service/core",
      "@identity-service/ui"
    ]
  },
  "resolutions": {
    "babel-loader": "8.1.0",
    "webpack": "4.44.2"
  }
}
