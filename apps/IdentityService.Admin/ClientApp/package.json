{
  "name": "@identity-service/admin",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "postinstall": "cp ./node_modules/oidc-client/dist/oidc-client.min.js ./public",
    "start": "craco start",
    "build": "cross-env CI=false craco build",
    "serve": "npx serve -s build",
    "test": "cross-env CI=true react-scripts test --collectCoverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6009 -s public",
    "build-storybook": "build-storybook -s public",
    "coverage": "open-cli coverage/lcov-report/index.html",
    "updates": "ncu"
  },
  "dependencies": {
    "@identity-service/core": "workspace:*",
    "@identity-service/ui": "workspace:*",
    "antd": "^5.7.0",
    "axios": "^1.2.0",
    "oidc-client": "^1.11.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "react-router-typesafe-routes": "^1.2.1",
    "react-scripts": "^5.0.1",
    "web-vitals": "^3.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@craco/types": "^7.1.0",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-docs": "^6.5.14",
    "@storybook/addon-essentials": "6.5.14",
    "@storybook/addon-links": "6.5.14",
    "@storybook/builder-webpack5": "6.5.14",
    "@storybook/manager-webpack5": "6.5.14",
    "@storybook/node-logger": "6.5.14",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "6.5.14",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^9.1.0",
    "craco-esbuild": "^0.5.2",
    "cross-env": "^7.0.3",
    "identity-obj-proxy": "^3.0.0",
    "open-cli": "^7.1.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.56.1",
    "storybook-addon-turbo-build": "^2.0.1",
    "storybook-design-token": "^2.8.0",
    "typescript": "^4.9.3"
  },
  "browserslist": {
    "production": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 3 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "^pnpm@8.0.0",
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.stories.{ts,tsx}",
      "!**/*.type.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 15,
        "functions": 40,
        "lines": 36,
        "statements": 30
      }
    },
    "coveragePathIgnorePatterns": [
      "index",
      "reportWebVitals",
      "react-app-env.d",
      "store.ts",
      "__snapshots__"
    ],
    "transformIgnorePatterns": [
      "@identity-service/core",
      "@identity-service/ui"
    ]
  },
  "cracoConfig": "config/craco.config.ts"
}
