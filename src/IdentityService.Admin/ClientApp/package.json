{
  "name": "@identity-service/admin",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env CI=false react-scripts build",
    "test": "cross-env CI=true react-scripts test --collectCoverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "coverage": "open-cli coverage/lcov-report/index.html",
    "updates": "ncu",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@identity-service/core": "workspace:*",
    "@identity-service/ui": "workspace:*",
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.5",
    "@storybook/addon-essentials": "^6.3.5",
    "@storybook/addon-links": "^6.3.5",
    "@storybook/node-logger": "^6.3.5",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "babel-loader": "8.1.0",
    "cross-env": "^7.0.3",
    "open-cli": "^7.0.0",
    "sass": "^1.36.0",
    "storybook-design-token": "^1.2.0",
    "typescript": "~4.3.5"
  },
  "browserslist": {
    "production": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 3 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.stories.{ts,tsx}",
      "!**/*.type.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coveragePathIgnorePatterns": [
      "index",
      "reportWebVitals",
      "react-app-env.d",
      "store.ts",
      "__snapshots__"
    ],
    "transformIgnorePatterns": [
      "@identity-service/core",
      "@identity-service/ui"
    ]
  },
  "resolutions": {
    "babel-loader": "8.1.0",
    "webpack": "4.44.2"
  }
}
